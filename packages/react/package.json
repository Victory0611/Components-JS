{
  "version": "0.1.0",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/components-react.esm.js"
    }
  },
  "typings": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "tsdx watch --format esm",
    "start": "tsdx watch --format esm",
    "build": "tsdx build --format esm",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "name": "@livekit/components-react",
  "author": "LiveKit",
  "module": "dist/components-react.esm.js",
  "size-limit": [
    {
      "path": "dist/nextjs.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/nextjs.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.12",
    "husky": "^8.0.1",
    "next": "^12.1.5",
    "postcss": "^8.4.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@livekit/components-core": "0.1.0",
    "@livekit/react-core": "^1.0.1",
    "livekit-client": "^1.2.10",
    "react-aria": "^3.19.0"
  }
}
